<%-include("partials/header");-%>
<body class="addNewCourse">
    <link rel="stylesheet" href="/css/styles.css" type="text/css">
    <h1>Add New Course</h1>


    <form action="/addNewCourse" method="POST">
    <div>

        <div>
            <h3>Course Name</h3>
            <input type="text" placeholder="Course Name" name="courseName">
        </div>
    
        <br><br>

        <div>
            <h3>Course Code</h3>
            <input type="text" placeholder="Course Code" name="courseCode">
        </div>

        <br><br>

        <div>
            <h3>Year Offered</h3>
            <input type="text" placeholder="Course Code" name="year">
        </div>

        <br><br>

        <div>
            <h3>Semester</h3>
            <input type="text" placeholder="Course Code" name="sem">
        </div>

        <br><br>

        <div>
            <button type="submit" >Confirm</button>
        </div>   
    </div>
    </form>
    <h3><%=message%></h3>
    <!-- <<script src="/js/index.js"></script> -->
</body>
</html>